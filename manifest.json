{
  "manifest_version": 2,
  "name": "Password Auth",
  "description": "password manager",
  "version": "1.0",
  "background": {
    "scripts": ["./JS/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "./imgs/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./JS/content.js"]
    }
  ],
  "permissions": ["activeTab", "storage"]
}
